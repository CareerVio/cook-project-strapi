{
  "kind": "collectionType",
  "collectionName": "redeems",
  "info": {
    "singularName": "redeem",
    "pluralName": "redeems",
    "displayName": "Redeem",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "redeems"
    },
    "totalPoints": {
      "type": "float",
      "required": true
    },
    "qrCode": {
      "type": "text",
      "required": true,
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "approved",
        "rejected"
      ],
      "required": true,
      "default": "pending"
    },
    "productJsonArray": {
      "type": "text"
    },
    "invoice": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::invoice.invoice",
      "mappedBy": "redeem"
    }
  }
}
