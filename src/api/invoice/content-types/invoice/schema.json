{
  "kind": "collectionType",
  "collectionName": "invoices",
  "info": {
    "singularName": "invoice",
    "pluralName": "invoices",
    "displayName": "Invoice",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "amount": {
      "type": "float",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid"
      ],
      "default": "paid",
      "required": true
    },
    "transferImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "redeem": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::redeem.redeem",
      "inversedBy": "invoice"
    },
    "productJsonArray": {
      "type": "text"
    },
    "invoiceNumber": {
      "type": "string"
    }
  }
}
