{
  "kind": "collectionType",
  "collectionName": "history_machines",
  "info": {
    "singularName": "history-machine",
    "pluralName": "history-machines",
    "displayName": "Transaction",
    "description": "Records for the history machine actions"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "type": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "time": {
      "type": "time"
    },
    "serialNumber": {
      "type": "string",
      "required": true
    },
    "point": {
      "type": "integer",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "quantity": {
      "type": "integer"
    },
    "cabinet": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::recycle-machine.recycle-machine"
    },
    "detail": {
      "type": "text"
    },
    "status": {
      "type": "string"
    },
    "point_earned": {
      "type": "integer"
    }
  }
}
