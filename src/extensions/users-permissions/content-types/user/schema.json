{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "redeems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::redeem.redeem",
      "mappedBy": "customer"
    },
    "lineId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "fullName": {
      "type": "string",
      "required": false
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Null"
      ],
      "required": true,
      "default": "Null"
    },
    "address": {
      "type": "text",
      "required": false
    },
    "cardID": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 13,
      "maxLength": 13,
      "regex": "^[1-9]\\d{12}$"
    },
    "telNumber": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 10,
      "maxLength": 10,
      "regex": "^0\\d{9}$"
    },
    "photoImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "userType": {
      "type": "enumeration",
      "enum": [
        "shop",
        "customer"
      ],
      "required": true
    },
    "shop": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shop.shop",
      "inversedBy": "user"
    },
    "point": {
      "type": "integer",
      "required": true,
      "default": 50
    },
    "cardIdImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "taxId": {
      "type": "string"
    },
    "companyName": {
      "type": "string"
    }
  }
}
